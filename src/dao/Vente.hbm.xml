<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
<class name="dao.Vente" table="VENTE" >
	<id name="id_vente" column="ID_VENTE"  >
		<generator class="native"></generator>
	</id>
	<property name="id_ordre_vente" column="ID_ORDRE_VENTE"></property>
	<property name="mode_paiement" column="MODE_PAIMENENT"></property>
</class>

</hibernate-mapping>